plot(anno,casi, xlab ="anno (a partire dal 1984)", ylab = "casi",
main = "Sviluppo  Aids")
plot(anno,casi, xlab = "anno (a partire dal 1984)", ylab = "casi",
main = "Sviluppo Aids",log="xy") # scale logaritmica
x= log10(anno)
y=log10(casi)
plot(x,y)
lm(y~x)
abline(lm(y~x))
plot(anno,casi);a=lm(y~x)$coefficients[2]#pendenza
b=lm(y~x)$coefficients[1]#intercetta
c=10^b;d=a;curve(10^b*(x)^a, add=T)
